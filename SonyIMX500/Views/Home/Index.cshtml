            if (expanded == 'false' || expanded == undefined) {
                $(item).removeClass('fa-angle-double-down');
                $(item).addClass('fa-angle-double-up');
            } else {
                $(item).removeClass('fa-angle-double-up');
                $(item).addClass('fa-angle-double-down');
            }
